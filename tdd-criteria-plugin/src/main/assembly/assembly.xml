<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/2.2.1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/2.2.1 http://maven.apache.org/xsd/assembly-2.2.1.xsd">
	<id>tdd-criteria-plugin</id>
	<formats>
		<format>jar</format>
	</formats>
	<dependencySets>
		<dependencySet>
			<includes>
				<include>*:jar:*</include>
			</includes>
			<excludes>
				<exclude>*:sources</exclude>
			</excludes>
		</dependencySet>
	</dependencySets>
	<fileSets>
		<fileSet>
			<outputDirectory>/tdd-criteria-plugin</outputDirectory>
		</fileSet>
	</fileSets>
	<includeBaseDirectory>true</includeBaseDirectory>
</assembly>